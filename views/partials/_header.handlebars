    <div class="navbar">
      <div class="navbar__back">
        <button class="navbar__back--button">
          <svg class="back-arrow" transform="rotate(90)">
            <defs>
              <linearGradient id="grad-back" x1="100%" y1="0%" x2="0%" y2="0%" spreadMethod="pad">
                <stop offset="0%" stop-color="#af5300" stop-opacity="1" />
                <stop offset="100%" stop-color="#f5851f" stop-opacity="1" />
